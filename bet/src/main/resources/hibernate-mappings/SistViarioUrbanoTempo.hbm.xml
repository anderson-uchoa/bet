<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="lps.bet.variabilidades.tiposDados"
>
    <class name="SistViarioUrbanoTempo" table="VAR_TEMPO">

        <id name="tempoID" column="tempoID" type="int">
            <generator class="increment"></generator>
        </id>

        <property name="tempoMaxIntegracao" type="int" update="true" insert="true" column="tempoMaxIntegracao"/>
		
    </class>

</hibernate-mapping>
