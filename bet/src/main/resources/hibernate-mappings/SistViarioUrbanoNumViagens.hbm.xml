<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="lps.bet.variabilidades.tiposDados">
	<class name="SistViarioUrbanoNumViagens" table="var_num_viagens">
		<id name="numViagensID" type="int" column="numViagensId">
			<generator class="increment" />
		</id>

		<property name="numViagens" type="int" update="true" insert="true" column="numViagens" />
	</class>
</hibernate-mapping>
