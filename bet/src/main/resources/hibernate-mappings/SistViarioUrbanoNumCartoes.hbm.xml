<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="lps.bet.variabilidades.tiposDados">
	<class name="SistViarioUrbanoNumCartoes" table="var_num_cartoes">
		<id name="numCartoesID" type="int" column="numCartoesId">
			<generator class="increment" />
		</id>

		<property name="limiteCartoes" type="int" update="true" insert="true" column="limiteCartoes" />
	</class>
</hibernate-mapping>
