<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="lps.bet.variabilidades.tiposDados">
	<class name="LinhaIntegrada" table="var_linha_integrada">

		<id name="integracaoID" type="int" column="integracaoId">
			<generator class="increment" />
		</id>

		<property name="linhaIntegradaID" type="int" update="true" insert="true" column="linhaIntegradaId" />

		<property name="linhaOriginalID" type="int" update="true" insert="true" column="linhaOriginalId" />
	</class>
</hibernate-mapping>
